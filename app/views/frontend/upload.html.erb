  <body>

    <div class="site-wrapper">

      <div class="site-wrapper-inner">

        <div class="cover-container">

          <div class="masthead clearfix">
            <div class="inner">
              <%= link_to '/', :title => "some_link" do %>
                <h3 class="masthead-brand">senneku</h3>
              <% end %>
            </div>
          </div>

          <div class="inner cover" id="primary-content">
